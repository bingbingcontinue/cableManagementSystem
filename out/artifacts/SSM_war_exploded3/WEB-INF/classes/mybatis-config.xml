<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <!-- paceage: 为某个包下的所有类批量起别名  -->
    <!-- name:指定包名，为当前包及以下所有后代包的每一个类都起一个默认别名(类名小写)   -->
    <typeAliases>
        <package name="com.cms.pojo"/>
    </typeAliases>



    <mappers>
        <mapper resource="com/cms/dao/BookMapper.xml"></mapper>
        <mapper resource="com/cms/dao/CableMapper.xml"></mapper>
        <mapper resource="com/cms/dao/MaintenanceMapper.xml"></mapper>
        <mapper resource="com/cms/dao/InterfaceMapper.xml"></mapper>
    </mappers>




</configuration>